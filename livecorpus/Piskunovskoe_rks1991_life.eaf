    <TIER ANNOTATOR="lenches" DEFAULT_LOCALE="ru"
        LINGUISTIC_TYPE_REF="utterance" PARTICIPANT="@rks1991m" TIER_ID="text@rks1991m">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1"
                TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>я учился до девятого класса / у нас есть была программа что= чтоб перейти в десятый в одиннадцатый но я не пошел принципиально</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3"
                TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>потому что ну / учась в колледже / где я учился мы проходили программу десятого одиннадцатого класса </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4"
                TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>то есть зачем мне тратить два года / если я могу проучиться и так / получить ну / такое ну / десятое одиннадцатое образование и в то же время учиться на / свое / профессиональное образование / автомеханик</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>то есть я как человек я люблю ковыряться в машинах / ковыряться ну / в / то что мне интересно в технике в любой</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>то есть у меня была машина первая ВАЗ двадцать один ноль шесть / я еще помню приехал до родственника</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>я её первый раз сам перебрал</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>то есть мы выра= вытащили / как говорит сердце машины /это двигатель / двигатель</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9"
                TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>мы его разобрали / и я так смотрю и думаю блин а как же его собирать / и мы так подумали ну как-то надо / и мы раз и собрали и ну / идеально все работало то есть</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10"
                TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>я горжусь то что</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11"
                TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts21">
                <ANNOTATION_VALUE>школа / школа вообще вот она / знаешь дает / какой-то определенный фундамент</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12"
                TIME_SLOT_REF1="ts20" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>то есть / дает тебе / эээ познание чего ты хочешь и чего ты можешь</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
